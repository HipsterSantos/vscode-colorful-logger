{
  "name": "colorful-logger-vscode",
  "displayName": "Colorful Logger Helper",
  "description": "In-editor log previews and error diagnostics for @hipstersantos/colorful-logger",
  "version": "0.0.2",
  "publisher": "hipstersantos",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other",
    "Debuggers"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "javascript",
        "extensions": [
          ".js"
        ],
        "aliases": [
          "JavaScript"
        ]
      },
      {
        "id": "typescript",
        "extensions": [
          ".ts"
        ],
        "aliases": [
          "TypeScript"
        ]
      }
    ]
  },
  "dependencies": {
    "@hipstersantos/colorful-logger": "^1.4.0"
  },
  "devDependencies": {
    "vscode": "^1.1.37"
  }
}
